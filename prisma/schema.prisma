generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id              String   @id @default(uuid())
  name            String
  description     String
  longDescription String?
  techStack       String[]
  url             String
  repoUrl         String?
  imageUrl        String
  role            String[]
  isFeatured      Boolean  @default(false)
  order           Int      @default(0)
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
